{
    "name": "codewithuali/laradoc",
    "description": "Intelligent Laravel project documentation generator with AI-powered chatbot",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Umair Ali",
            "email": "me@umair.lu"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "league/commonmark": "^2.0",
        "meilisearch/meilisearch-php": "^0.35.0",
        "spatie/laravel-permission": "^5.0",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Laradoc\\Laradoc\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laradoc\\Laradoc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laradoc\\Laradoc\\LaradocServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 